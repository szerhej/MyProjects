<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config/>

	<bean name="BasicConfig" class="fg.eternity.config.BasicConfig" />

	<bean id="figureSet"
		  class="fg.eternity.spring.FigureSet">
		<property name="figureMap" ref="figureMap"/>
	</bean>

	<bean id="mainRoute"
		class="fg.eternity.spring.RouteDefinition">
		<property name="route">
			<list>
				<ref bean="endless"/>
				<value>{A16},{A15},{B16},{B15},{C16},{A14},{D16},{C15},{B14},{E16},{D15},{F16},{E15},{D14},{G16},{F15},{E14},{H16},{G15},{F14},{I16},{H15},{G14},{J16},{I15},{H14},{K16},{J15},{I14},{L16},{K15},{J14},{M16},{L15},{K14},{N16},{M15},{L14},{O16},{N15},{M14},{P16},{O15},{P15},{O14},{P14}</value>
				<value>{P13},{O13},{N13},{P12},{O12},{N12},{P11},{O11},{N11},{P10},{O10},{N10},{P9},{O9},{N9},{P8},{O8},{N8},{P7},{O7},{N7}</value>
				<ref bean="once"/>
				<ref bean="logger"/>
				<value>{P6},{O6},{N6},{P5},{O5},{N5},{P4},{O4},{N4},{P3},{O3},{P2},{O2},{N2},{P1},{O1},{N1}</value>
				<value>{M13},{L13},{K13},{J13},{I13}</value>
				<value>{M12},{M11},{M10},{M9}</value>
				<value>{L12},{K12},{J12},{I12}</value>
				<value>{L11},{L10},{L9}</value>
				<value>{K11},{J11},{I11}</value>
				<value>{K10},{K9}</value>
				<value>{J10},{I10},{J9},{I9}</value>
				<value>{M8},{L8},{K8},{J8}</value>
				<value>{A1},{A2},{B1},{B2},{C2},{C1},{B3},{A3}</value>
				<value>{M7},{M6},{M5},{M4},{M3},{M2},{M1}</value>
				<value>{H13},{G13},{F13},{E13},{D13},{C13},{B13},{A13}</value>
				<value>{L7},{L6},{L5},{L4},{L3},{L2},{L1}</value>
				<value>{H12},{G12},{F12},{E12},{D12},{C12},{B12},{A12}</value>
				<value>{K7},{K6},{K5},{K4},{K3},{K2},{K1}</value>
				<value>{H11},{G11},{F11},{E11},{D11},{C11},{B11},{A11}</value>
				<value>{J7},{J6},{J5},{J4},{J3},{J2},{J1}</value>
				<value>{H10},{G10},{F10},{E10},{D10},{C10},{B10},{A10}</value>
				<value>{I7},{I6},{I5},{I4},{I3},{I2},{I1}</value>
				<value>{H9},{G9},{F9},{E9},{D9},{C9},{B9},{A9}</value>
				<ref bean="finalFork"/>
			</list>
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>
	
	<bean id="finalFork"
		class="fg.eternity.plan.PlanFork">
		<property name="forkList">
			<list>
				<ref bean="final1"/>
				<ref bean="final2"/>
				<ref bean="final3"/>
				<ref bean="final6"/>
				<ref bean="final7"/>
			</list>
		</property>
	</bean>
	

	<bean id="final1"
		class="fg.eternity.spring.RouteDefinition">
		<property name="route">
			<list>
				<value>{H8},{G8},{F8},{E8},{D8},{C8},{B8},{H7},{H6},{H5},{H4},{H3},{H2},{G7},{F7}</value>
				<bean class="fg.eternity.plan.PlanLogger">
					<property name="first">
						<ref bean="mainRoute" />
					</property>
					<property name="loggerName" value="GOAL"/>
					<property name="planSet">
						<ref bean="mainTable" />
					</property>
				</bean>
				<value>{E7},{D7},{C7},{B7},{G6},{G5},{G4},{G3},{G2},{F6},{E6},{D6},{C6},{B6},{F5},{F4},{F3},{F2},{E5},{D5},{C5},{B5},{E4},{E3},{E2},{D4},{C4}</value>
				<ref bean="bingo"/>
			</list>
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>
	
	<bean id="final2"
		class="fg.eternity.spring.RouteDefinition">
		<property name="route">
			<list>
				<value>{H8},{H7},{H6},{H5},{H4},{H3},{H2},{G8},{F8},{E8},{D8},{C8},{B8},{G7},{G6}</value>
				<bean class="fg.eternity.plan.PlanLogger">
					<property name="first">
						<ref bean="mainRoute" />
					</property>
					<property name="loggerName" value="GOAL"/>
					<property name="planSet">
						<ref bean="mainTable" />
					</property>
				</bean>
				<value>{G5},{G4},{G3},{G2},{F7},{E7},{D7},{C7},{B7},{F6},{E6},{D6},{C6},{B6},{F5},{F4},{F3},{F2},{E5},{D5},{C5},{B5},{E4},{E3},{E2},{D4},{C4}</value>
				<ref bean="bingo"/>
			</list>
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>

	<bean id="final3"
		class="fg.eternity.spring.RouteDefinition">
		<property name="route">
			<list>
				<value>{H8},{H7},{G8},{H6},{G7},{F8},{H5},{G6},{F7},{E8},{H4},{G5},{F6},{E7},{D8}</value>
				<bean class="fg.eternity.plan.PlanLogger">
					<property name="first">
						<ref bean="mainRoute" />
					</property>
					<property name="loggerName" value="GOAL"/>
					<property name="planSet">
						<ref bean="mainTable" />
					</property>
				</bean>
				<value>{H3},{G4},{F5},{E6},{D7},{C8},{H2},{G3},{F4},{E5},{D6},{C7},{B8},{G2},{F3},{E4},{D5},{C6},{B7},{F2},{E3},{D4},{C5},{B6},{E2},{D3},{C4},{B5}</value>
				<ref bean="bingo"/>
			</list>
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>

	<bean id="final6"
		class="fg.eternity.spring.RouteDefinition">
		<property name="route">
			<list>
				<value>{H8},{G8},{F8},{E8},{D8},{C8},{B8},{H7},{G7},{F7},{E7},{D7},{C7},{B7},{H6}</value>
				<bean class="fg.eternity.plan.PlanLogger">
					<property name="first">
						<ref bean="mainRoute" />
					</property>
					<property name="loggerName" value="GOAL"/>
					<property name="planSet">
						<ref bean="mainTable" />
					</property>
				</bean>
				<value>{G6},{F6},{E6},{D6},{C6},{B6},{H5},{G5},{F5},{E5},{D5},{C5},{B5},{H4},{G4},{F4},{E4},{D4},{C4}</value>
				<ref bean="bingo"/>
			</list>
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>

	<bean id="final7"
		class="fg.eternity.spring.RouteDefinition">
		<property name="route">
			<list>
				<value>{H8},{H7},{H6},{H5},{H4},{H3},{H2},{G8},{G7},{G6},{G5},{G4},{G3},{G2},{F8}</value>
				<bean class="fg.eternity.plan.PlanLogger">
					<property name="first">
						<ref bean="mainRoute" />
					</property>
					<property name="loggerName" value="GOAL"/>
					<property name="planSet">
						<ref bean="mainTable" />
					</property>
				</bean>
				<value>{F7},{F6},{F5},{F4},{F3},{F2},{E8},{E7},{E6},{E5},{E4},{E3},{E2},{D8},{D7},{D6},{D5},{D4},{D3}</value>
				<ref bean="bingo"/>
			</list>
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>
	
	<bean id="fieldSet"
		class="fg.eternity.spring.FieldSet">
		<constructor-arg >
			<ref bean="config" />
		</constructor-arg>
	</bean>
	
	<bean id="config"
          class="fg.eternity.bo.Config" init-method="init">
		<property name="dimX" value="16"/>
		<property name="dimY" value="16"/>
		<property name="patternCount" value="22"/>
	</bean>
	
	<bean id="mainTable"
          class="fg.eternity.plan.PlanSet" init-method="init">
		<property name="config">
			<ref bean="config" />
		</property>
		<property name="fieldSet">
			<ref bean="fieldSet" />
		</property>
		<property name="figureSet">
			<ref bean="figureSet" />
		</property>
		<property name="defaults"   >
			<value>
				<![CDATA[I8<fkrf>^1,N3<kpfu>^2,N14<duol>^3,C14<pvtv>^2,C3<svck>^2]]>
			</value>
		</property>
	</bean>
	
	<bean id="logger"
		class="fg.eternity.plan.PlanLogger">
		<property name="first">
			<ref bean="mainRoute" />
		</property>
		<property name="loggerName" value="LOGGER"/>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>

	<bean id="bingo"
		class="fg.eternity.plan.Bingo">
		<property name="first">
			<ref bean="mainRoute" />
		</property>
		<property name="planSet">
			<ref bean="mainTable" />
		</property>
	</bean>
	
	<bean id="endless"
		class="fg.eternity.plan.PlanEndless">
	</bean>

	<bean id="once"
		class="fg.eternity.plan.PlanOnce">
		<property name="first">
			<ref bean="mainRoute" />
		</property>
	</bean>


</beans>
